<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="YsmStore.Controls.NavigationBar">
  <ContentView.Content>
        <Frame Style="{StaticResource RoundedFrame}" BackgroundColor="{StaticResource Color2}">
            <StackLayout>
                <Grid ColumnDefinitions="1*,3*,1*" RowDefinitions="70">
                    <ImageButton x:Name="backButton" Source="back.png" BackgroundColor="#00000000" Margin="20" Clicked="BackButton_Tapped"/>
                    <Image Source="Logo.png" Grid.Column="1"/>
                    <Frame x:Name="rightButtonFrame" Grid.Column="2" Padding="0" BackgroundColor="#00000000"/>
                </Grid>
                <Frame x:Name="contentFrame" Padding="0" BackgroundColor="#00000000"/>
            </StackLayout>
        </Frame>
    </ContentView.Content>
</ContentView>